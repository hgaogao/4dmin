{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["src/**/*.vue", "src/**/*.ts", "types/**/*.d.ts", "./*.d.ts"], // include的作用是指定哪些文件需要被编译
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },

  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
